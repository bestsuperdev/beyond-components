{
  "name": "react-component-boilerplate",
  "version": "0.1.0@beta",
  "description": "a react component boilerplate",
  "main": "lib/index.js",
  "style": "lib/index.css",
  "scripts": {
    "start": "node ./server.js",
    "mock": "cd ./mock & json-server --watch db.json --routes routes.json --port 9001",
    "lint": "eslint ./src",
    "test": "karma start  --config karma.conf.js",
    "clear": "rimraf lib",
    "build": "npm run clear && tsc -d -p src && node ./gulpfile.js",
    "clear:example": "rimraf dist",
    "build:example": "npm run clear:example && cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "dependencies": {
    "@types/classnames": "0.0.32",
    "@types/react": ">=0.14.0",
    "@types/react-addons-css-transition-group": "^15.0.0",
    "@types/react-dom": ">=0.14.0",
    "beyond-lib": "^1.0.1",
    "classnames": "^2.2.5",
    "react": ">=0.14.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "@types/react": "^0.14.50",
    "@types/react-dom": "^0.14.19",
    "autoprefixer": "^6.1.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.0",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.6.5",
    "json-server": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "less-plugin-autoprefix": "^1.5.1",
    "opn": "^4.0.2",
    "postcss-clearfix": "^1.0.0",
    "postcss-filter-gradient": "^0.2.4",
    "postcss-loader": "^1.1.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.2.2",
    "typescript": "^2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bestsuperdev/react-component-boilerplate.git"
